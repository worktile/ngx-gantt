{
  "$schema": "./node_modules/@angular/cli/lib/config/schema.json",
  "version": 1,
  "newProjectRoot": "projects",
  "projects": {
    "gantt": {
      "projectType": "library",
      "root": "packages/gantt",
      "sourceRoot": "packages/gantt/src",
      "prefix": "ngx",
      "architect": {
        "build": {
          "builder": "@angular-devkit/build-angular:ng-packagr",
          "options": {
            "tsConfig": "packages/gantt/tsconfig.lib.json",
            "project": "packages/gantt/ng-package.json"
          },
          "configurations": {
            "production": {
              "tsConfig": "packages/gantt/tsconfig.lib.prod.json"
            }
          }
        },
        "test": {
          "builder": "@angular-devkit/build-angular:karma",
          "options": {
            "main": "packages/gantt/src/test.ts",
            "tsConfig": "packages/gantt/tsconfig.spec.json",
            "codeCoverage": true,
            "karmaConfig": "packages/gantt/karma.conf.js"
          }
        },
        "lint": {
          "builder": "@angular-eslint/builder:lint",
          "options": {
            "lintFilePatterns": [
              "packages/gantt/**/*.ts",
              "packages/gantt/**/*.html"
            ]
          }
        }
      }
    },
    "example": {
      "projectType": "application",
      "schematics": {
        "@schematics/angular:component": {
          "style": "scss"
        }
      },
      "root": "example",
      "sourceRoot": "example/src",
      "prefix": "example",
      "architect": {
        "build": {
          "builder": "@angular-devkit/build-angular:browser",
          "options": {
            "outputPath": "dist/example",
            "index": "example/src/index.html",
            "main": "example/src/main.ts",
            "polyfills": "example/src/polyfills.ts",
            "tsConfig": "example/tsconfig.app.json",
            "assets": [
              "example/src/favicon.ico",
              "example/src/assets"
            ],
            "styles": [
              "example/src/styles.scss"
            ],
            "scripts": [],
            "vendorChunk": true,
            "extractLicenses": false,
            "buildOptimizer": false,
            "sourceMap": true,
            "optimization": false,
            "namedChunks": true,
            "allowedCommonJsDependencies": ["docsearch.js"]
          },
          "configurations": {
            "production": {
              "fileReplacements": [
                {
                  "replace": "example/src/environments/environment.ts",
                  "with": "example/src/environments/environment.prod.ts"
                }
              ],
              "optimization": true,
              "outputHashing": "all",
              "sourceMap": false,
              "namedChunks": false,
              "extractLicenses": true,
              "vendorChunk": false,
              "buildOptimizer": true,
              "budgets": [
                {
                  "type": "initial",
                  "maximumWarning": "2mb",
                  "maximumError": "5mb"
                },
                {
                  "type": "anyComponentStyle",
                  "maximumWarning": "6kb",
                  "maximumError": "10kb"
                }
              ]
            }
          },
          "defaultConfiguration": ""
        },
        "serve": {
          "builder": "@angular-devkit/build-angular:dev-server",
          "options": {
            "buildTarget": "example:build"
          },
          "configurations": {
            "production": {
              "buildTarget": "example:build:production"
            }
          }
        },
        "extract-i18n": {
          "builder": "@angular-devkit/build-angular:extract-i18n",
          "options": {
            "buildTarget": "example:build"
          }
        }
      }
    }
  },
  "cli": {
    "analytics": "7595d21d-c10d-4b06-8050-b8e56319939c",
    "schematicCollections": [
      "@angular-eslint/schematics"
    ]
  }
}
