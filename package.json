{
  "name": "@worktile/gantt",
  "version": "0.0.50",
  "description": "A modern and powerful gantt chart component for Angular",
  "keywords": [
    "gantt",
    "angular gantt",
    "ngx gantt"
  ],
  "scripts": {
    "ng": "ng",
    "start": "docgeni serve",
    "build:docs": "docgeni build",
    "build": "npm run build:lib && npm run build:lib:scss",
    "build:lib": "ng build gantt --prod",
    "build:lib:scss": "scss-bundle -c scss-bundle.config.json",
    "test": "ng test",
    "lint": "ng lint",
    "e2e": "ng e2e",
    "release": "standard-version",
    "pub-only": "cd dist/gantt && npm publish --access=public",
    "pub": "npm run build && npm run pub-only",
    "report-coverage": "cat ./coverage/gantt/lcov.info | coveralls",
    "changelog": "conventional-changelog -p angular -i CHANGELOG.md -s --pkg ./packages/gantt/package.json"
  },
  "dependencies": {
    "@angular/animations": "~10.2.5",
    "@angular/cdk": "^8.2.0",
    "@angular/common": "~10.2.5",
    "@angular/compiler": "~10.2.5",
    "@angular/core": "~10.2.5",
    "@angular/forms": "~10.2.5",
    "@angular/platform-browser": "~10.2.5",
    "@angular/platform-browser-dynamic": "~10.2.5",
    "@angular/router": "~10.2.5",
    "@types/es6-promise": "^3.3.0",
    "date-fns": "^2.14.0",
    "html2canvas": "1.0.0-rc.7",
    "rxjs": "~6.6.3",
    "tslib": "^1.10.0",
    "zone.js": "~0.10.2"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "~0.1002.3",
    "@angular-devkit/build-ng-packagr": "~0.1002.3",
    "@angular/cli": "~10.2.3",
    "@angular/compiler-cli": "~10.2.5",
    "@angular/language-service": "~10.2.5",
    "@commitlint/cli": "^7.5.2",
    "@commitlint/config-conventional": "^7.5.0",
    "@docgeni/cli": "0.3.0",
    "@docgeni/template": "0.3.0",
    "@types/jasmine": "~3.3.8",
    "@types/jasminewd2": "~2.0.3",
    "@types/node": "^12.11.1",
    "codelyzer": "^5.1.2",
    "concurrently": "^5.2.0",
    "conventional-changelog-cli": "^2.0.12",
    "coveralls": "^3.0.6",
    "fs-extra": "^9.0.0",
    "jasmine-core": "~3.4.0",
    "jasmine-spec-reporter": "~4.2.1",
    "karma": "~4.1.0",
    "karma-chrome-launcher": "~2.2.0",
    "karma-coverage-istanbul-reporter": "~2.0.1",
    "karma-jasmine": "~2.0.1",
    "karma-jasmine-html-reporter": "^1.4.0",
    "ng-packagr": "^10.1.1",
    "path": "^0.12.7",
    "pretty-quick": "^1.10.0",
    "protractor": "~5.4.0",
    "scss-bundle": "^3.1.2",
    "standard-version": "^8.0.1",
    "ts-node": "~7.0.0",
    "tslint": "~5.15.0",
    "typescript": "~4.0.8"
  },
  "husky": {
    "hooks": {
      "commit-msg": "commitlint -E HUSKY_GIT_PARAMS",
      "pre-commit": "pretty-quick --staged"
    }
  },
  "standard-version": {
    "bumpFiles": [
      "package.json",
      "package-lock.json",
      "packages/gantt/package.json"
    ],
    "header": "Changelog\nAll notable changes to ngx-gantt will be documented in this file.\n",
    "tagPrefix": "",
    "skip": {
      "tag": true,
      "commit": true
    }
  },
  "main": ".docgenirc.js",
  "directories": {
    "doc": "docs",
    "example": "example"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/worktile/ngx-gantt.git"
  },
  "author": "walkerkay001",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/worktile/ngx-gantt/issues"
  },
  "homepage": "https://github.com/worktile/ngx-gantt#readme"
}