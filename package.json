{
  "name": "ngx-gantt",
  "version": "0.0.0",
  "scripts": {
    "ng": "ng",
    "start:docs": "docgeni serve",
    "start": "concurrently \"npm run start:docs\" \"ng serve\"",
    "build": "ng build gantt",
    "test": "ng test",
    "lint": "ng lint",
    "e2e": "ng e2e",
    "pub-only": "cd dist/gantt && npm publish --access=public",
    "pub": "npm run build && npm run pub-only",
    "changelog": "conventional-changelog -p angular -i CHANGELOG.md -s --pkg ./packages/gantt/package.json"
  },
  "dependencies": {
    "@angular/animations": "~8.2.14",
    "@angular/cdk": "^8.2.0",
    "@angular/common": "~8.2.14",
    "@angular/compiler": "~8.2.14",
    "@angular/core": "~8.2.14",
    "@angular/forms": "~8.2.14",
    "@angular/platform-browser": "~8.2.14",
    "@angular/platform-browser-dynamic": "~8.2.14",
    "@angular/router": "~8.2.14",
    "date-fns": "^2.14.0",
    "rxjs": "~6.4.0",
    "tslib": "^1.10.0",
    "zone.js": "~0.9.1",
    "html2canvas": "^1.0.0-rc.5"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "~0.803.25",
    "@angular-devkit/build-ng-packagr": "~0.803.25",
    "@angular/cli": "~8.3.21",
    "@angular/compiler-cli": "~8.2.14",
    "@angular/language-service": "~8.2.14",
    "@commitlint/cli": "^7.5.2",
    "@commitlint/config-conventional": "^7.5.0",
    "@docgeni/cli": "0.1.0-alpha.7",
    "@docgeni/template": "0.1.0-alpha.7",
    "@types/jasmine": "~3.3.8",
    "@types/jasminewd2": "~2.0.3",
    "@types/node": "~8.9.4",
    "codelyzer": "^5.0.0",
    "concurrently": "^5.2.0",
    "jasmine-core": "~3.4.0",
    "jasmine-spec-reporter": "~4.2.1",
    "karma": "~4.1.0",
    "karma-chrome-launcher": "~2.2.0",
    "karma-coverage-istanbul-reporter": "~2.0.1",
    "karma-jasmine": "~2.0.1",
    "karma-jasmine-html-reporter": "^1.4.0",
    "ng-packagr": "^5.4.0",
    "pretty-quick": "^1.10.0",
    "protractor": "~5.4.0",
    "standard-version": "^7.1.0",
    "ts-node": "~7.0.0",
    "tsickle": "^0.37.0",
    "tslint": "~5.15.0",
    "typescript": "~3.5.3",
    "conventional-changelog-cli": "^2.0.12"
  },
  "husky": {
    "hooks": {
      "commit-msg": "commitlint -E HUSKY_GIT_PARAMS",
      "pre-commit": "pretty-quick --staged"
    }
  },
  "standard-version": {
    "bumpFiles": [
      "package.json",
      "package-lock.json",
      "packages/gantt/package.json"
    ],
    "header": "Changelog\nAll notable changes to ngx-gantt will be documented in this file.\n",
    "tagPrefix": "",
    "skip": {
      "tag": true,
      "commit": true
    }
  }
}
